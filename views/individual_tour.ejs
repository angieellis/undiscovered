<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&signed_in=true"></script>

<script>

var coordinate = $('#coordinates').html().split(",");

function initialize() {
  var mapOptions = {
    zoom: 10,
    center: new google.maps.LatLng(coordinate[1], coordinate[0]
)
  }
  var map = new google.maps.Map(document.getElementById('map-canvas'),
                                mapOptions);
  setMarkers(map, coordinate);
}

function setMarkers(map, locations) {
  console.log(coordinate)
  console.log("Google map is working")
  var shape = {
      coords: [1, 1, 1, 20, 18, 20, 18 , 1],
      type: 'poly'
  };
  var myLatLng = new google.maps.LatLng(coordinate[1], coordinate[0]
);
  var marker = new google.maps.Marker({
      position: myLatLng,
      map: map,
      shape: shape
  });
}

google.maps.event.addDomListener(window, 'load', initialize);

</script>
<div id="map-canvas"></div>
